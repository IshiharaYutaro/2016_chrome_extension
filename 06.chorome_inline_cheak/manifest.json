{
  "name": "search_chrome_line",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "検索したい文字をchromeの検索結果に挿入する拡張。",
  "content_scripts": [
    {
      "matches": ["https://www.google.co.jp/*"],
      "js": ["jquery.min.js","script.js"],
      "run_at" :"document_idle"
    }
  ],
  "icons": {
        "128": "img/icon_128.png",
        "48":  "img/icon_48.png",
        "16":  "img/icon_16.png"
    }
  }